{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2016",
    "rootDir": "src",
    "sourceMap": false,
    "alwaysStrict": true,
    "strictFunctionTypes": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    //"noUnusedLocals": true,
    //"noImplicitThis": true
    "noImplicitReturns": true
  },
  "exclude": [
    "node_modules"
  ]
}